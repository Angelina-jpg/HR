print('Введите трехзначное число')
d = int(input())
a = str(d % 10)
b = str(d // 10 % 10)
c = str(d // 100 % 10)
print(a + b + c)
print(a + c + b)
print(b + a + c)
print(b + c + a)
print(c + a + b)
print(c + b + a)



