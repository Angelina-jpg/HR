print('Введите число')
a = int(input())
b = int(input())
c = int(input())
d = int(input())
count = 0
g = 0
if a % 2 > 0:
    count  += 1
    g += a
if b % 2 > 0:
    count  += 1
    g += b
if c % 2 > 0:
    count  += 1
    g += c
if d % 2 > 0:
    count  += 1
    g += d
print(count, 'Количество нечетных чисел')
print(g, 'Сумма нечетных чисел')
